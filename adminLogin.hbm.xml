<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"org/hibernate/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.bargadss.Propozal.Admin.bean.AdminLogin" table="AdminLogin">
	
		<id column="SerialNo" name="serialNo" type="long" >
		<generator class="increment" />
		</id>
		<property column="UserName" name="userName" type="string" />
		<property column="Password" name="password" type="string" />
	</class>
</hibernate-mapping>

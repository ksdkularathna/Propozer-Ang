<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"org/hibernate/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.bargadss.Propozal.bean.ImageProcessing" table="ImageUpload">
	
		<id column="imageId" name="imageId" type="long" >
		<generator class="increment" />
		</id>
		
		<property column="image" name="image" type="binary" />
		
	</class>
</hibernate-mapping>
